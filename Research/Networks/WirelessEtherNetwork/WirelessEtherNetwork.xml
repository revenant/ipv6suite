<?xml version="1.0" encoding="iso-8859-1"?>
<!DOCTYPE netconf SYSTEM "../../Etc/netconf2.dtd">

<netconf debugChannel="debug.log:notice:xmlAddresses:UDPVidStrmSvr:WirelessEthernet">
  <global gHostDupAddrDetectTransmits="2"/>
  <local node="ms1">
    <interface name="wlan0">    
      <WirelessEtherInfo>
        <WEInfo WETxPower="4.0" WEShadowing = "off" WESignalStrengthMaxSample="10" />
      </WirelessEtherInfo>
    </interface>
  </local>
  <local node="ms2">
    <interface name="wlan0">
      <WirelessEtherInfo>
        <WEInfo WETxPower="4.0" />
      </WirelessEtherInfo>
    </interface>
  </local>
  <local node="ap1">
    <interface name="wlanap">
      <WirelessEtherInfo>
        <WEInfo WETxPower="4.0" />
      </WirelessEtherInfo>
    </interface>
  </local>
  <local node="ap2">
    <interface name="wlanap">    
      <WirelessEtherInfo>
        <WEInfo WETxPower="4.0" />
      </WirelessEtherInfo>
    </interface>
  </local>
  <local node="ap3">
    <interface name="wlanap">    
      <WirelessEtherInfo>
        <WEInfo WETxPower="4.0" />
      </WirelessEtherInfo>
    </interface>
  </local>
  <local node="ap4">
    <interface name="wlanap">    
      <WirelessEtherInfo>
        <WEInfo WETxPower="4.0" />
      </WirelessEtherInfo>
    </interface>
  </local>

  <misc>
    <ObjectMovement>
      <MovingNode NodeName="ms1" startTime="0">
        <move moveToX="400" moveToY="150" moveSpeed="2"/>
      </MovingNode>
    </ObjectMovement>
  </misc>
</netconf>
