#

# Name of target to be created (-o option)
TARGET = RSVP_TE

# User interface (uncomment one) (-u option)
# USERIF_LIBS=$(CMDENV_LIBS)
USERIF_LIBS=$(TKENV_LIBS)

# .ned or .h include paths with -I
INCLUDE_PATH= -I../IPv4 -I../IPv4d -I../MPLS -I../../Base -I../../Util -I../Generic

# misc additional object and library files to link
EXTRA_OBJS=

# object files from other directories to link with (wildcard needed to prevent "no such file *.o" errors)
EXT_DIR_OBJS=

# time stamps of other directories (used as dependency)
EXT_DIR_TSTAMPS= ../IPv4/.tstamp ../IPv4d/.tstamp ../MPLS/.tstamp ../../Base/.tstamp ../../Util/.tstamp ../Generic/.tstamp

# Additional libraries (-L option -l option)
LIBS=

#------------------------------------------------------------------------------
# Import generic settings from ../../inetconfig
include ../../inetconfig

#------------------------------------------------------------------------------

# subdirectories to recurse into
SUBDIRS= 

# object files in this directory
OBJS= IntServ_m.o RSVPPacket_m.o RSVPPathMsg_m.o RSVPResvMsg_m.o OspfTe.o RSVPApp.o RSVP.o TED.o

# header files generated (from msg files)
GENERATEDHEADERS= IntServ_m.h RSVPPacket_m.h RSVPPathMsg_m.h RSVPResvMsg_m.h

#------------------------------------------------------------------------------

$(TARGET): $(OBJS) subdirs Makefile .tstamp
	@# do nothing

.tstamp: $(OBJS)
	echo>.tstamp

$(OBJS) : $(GENERATEDHEADERS)


purify: $(OBJS) $(EXTRA_OBJS) $(EXT_DIR_TSTAMPS) subdirs Makefile
	purify $(CXX) $(LDFLAGS) $(OBJS) $(EXTRA_OBJS) $(EXT_DIR_OBJS) $(LIBS) -L$(OMNETPP_LIB_DIR) $(KERNEL_LIBS) $(USERIF_LIBS) $(SYS_LIBS_PURE) -o $(TARGET).pure

$(EXT_DIR_TSTAMPS):
	@echo
	@echo Error: $@ does not exist.
	@echo This means that at least the above dependency directory has not been built.
	@echo Maybe you need to do a top-level make?
	@echo
	@exit 1

.PHONY: subdirs $(SUBDIRS)

subdirs: $(SUBDIRS)

IntServ_m.o: IntServ_m.cc
	$(CXX) -c $(NEDCOPTS) IntServ_m.cc

IntServ_m.cc IntServ_m.h: IntServ.msg
	$(MSGC) $(INCLUDE_PATH) IntServ.msg

RSVPPacket_m.o: RSVPPacket_m.cc
	$(CXX) -c $(NEDCOPTS) RSVPPacket_m.cc

RSVPPacket_m.cc RSVPPacket_m.h: RSVPPacket.msg
	$(MSGC) $(INCLUDE_PATH) RSVPPacket.msg

RSVPPathMsg_m.o: RSVPPathMsg_m.cc
	$(CXX) -c $(NEDCOPTS) RSVPPathMsg_m.cc

RSVPPathMsg_m.cc RSVPPathMsg_m.h: RSVPPathMsg.msg
	$(MSGC) $(INCLUDE_PATH) RSVPPathMsg.msg

RSVPResvMsg_m.o: RSVPResvMsg_m.cc
	$(CXX) -c $(NEDCOPTS) RSVPResvMsg_m.cc

RSVPResvMsg_m.cc RSVPResvMsg_m.h: RSVPResvMsg.msg
	$(MSGC) $(INCLUDE_PATH) RSVPResvMsg.msg

OspfTe.o: OspfTe.cc
	$(CXX) -c $(COPTS) OspfTe.cc

RSVPApp.o: RSVPApp.cc
	$(CXX) -c $(COPTS) RSVPApp.cc

RSVP.o: RSVP.cc
	$(CXX) -c $(COPTS) RSVP.cc

TED.o: TED.cc
	$(CXX) -c $(COPTS) TED.cc


#doc: neddoc doxy

#neddoc:
#	opp_neddoc -a

#doxy: doxy.cfg
#	doxygen doxy.cfg

generateheaders: $(GENERATEDHEADERS)
	for i in $(SUBDIRS); do (cd $$i && $(MAKE) generateheaders) || exit 1; done

clean:
	rm -f *.o *_n.cc *_n.h *_m.cc *_m.h .tstamp
	rm -f *.vec *.sca
	for i in $(SUBDIRS); do (cd $$i && $(MAKE) clean); done

depend:
	$(MAKEDEPEND) $(INCLUDE_PATH) -- *.cc
	# $(MAKEDEPEND) $(INCLUDE_PATH) -fMakefile.in -- *.cc
	for i in $(SUBDIRS); do (cd $$i && $(MAKE) depend) || exit 1; done

makefiles:
	# recreate Makefile
	opp_makemake -f  -N -b /home/andras/IPv6SuiteWithINET -c /home/andras/IPv6SuiteWithINET/inetconfig -n -r -I../IPv4 -I../IPv4d -I../MPLS -I../../Base -I../../Util -I../Generic 
	for i in $(SUBDIRS); do (cd $$i && $(MAKE) makefiles) || exit 1; done

makefile-ins:
	# recreate Makefile.in
	opp_makemake -f -m  -N -b /home/andras/IPv6SuiteWithINET -c /home/andras/IPv6SuiteWithINET/inetconfig -n -r -I../IPv4 -I../IPv4d -I../MPLS -I../../Base -I../../Util -I../Generic 
	for i in $(SUBDIRS); do (cd $$i && $(MAKE) makefile-ins) || exit 1; done

# "re-makemake" and "re-makemake-m" are deprecated, historic names of the above two targets
re-makemake: makefiles
re-makemake-m: makefile-ins


