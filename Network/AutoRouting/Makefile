	opp_makemake -f -m  -N -b /home/ewu/IPv6SuiteWithINET -c /home/ewu/IPv6SuiteWithINET/inetconfig -n -r -I../IPv4 -I../../Base -I../../Util -I../Generic -I../IPv6 -I../ICMPv6 
	for i in $(SUBDIRS); do (cd $$i && $(MAKE) makefile-ins) || exit 1; done

