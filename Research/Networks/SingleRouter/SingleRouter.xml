<?xml version="1.0" encoding="iso-8859-1"?>
<!DOCTYPE netconf SYSTEM "../../../Etc/netconf2.dtd">
<netconf>
  <local node="router" routePackets="on">
    <interface name="eth0" AdvSendAdvertisements="on">
      <AdvPrefixList>
        <AdvPrefix AdvOnLinkFlag="on">3333:0:0:4444:0:0:0:0/64</AdvPrefix>
      </AdvPrefixList>
    </interface>

    <interface name="eth1" AdvSendAdvertisements="on">
      <AdvPrefixList>
        <AdvPrefix AdvOnLinkFlag="on">3333:0:0:1111:0:0:0:0/64</AdvPrefix>
      </AdvPrefixList>
    </interface>

    <interface name="eth2" AdvSendAdvertisements="on">
      <AdvPrefixList>
        <AdvPrefix AdvOnLinkFlag="on">AAAA:0:0:3333:0:0:0:0/64</AdvPrefix>
      </AdvPrefixList>
    </interface>
  </local>
</netconf>
